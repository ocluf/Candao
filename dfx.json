{
  "canisters": {
    "candao": {
      "build": "cargo build --target wasm32-unknown-unknown --package candao --release",
      "candid": "src/candao/src/candao.did",
      "wasm": "target/wasm32-unknown-unknown/release/candao.wasm",
      "type": "custom"
    },
    "candao_assets": {
      "dependencies": [
        "candao"
      ],
      "source": [
        "src/candao_assets/out"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "dfx": "0.8.1",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}